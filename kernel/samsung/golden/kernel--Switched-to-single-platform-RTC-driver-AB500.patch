From dc81e3187372162de63540d3c2632a1457e572d4 Mon Sep 17 00:00:00 2001
From: Stefan Berger <s.berger81@gmail.com>
Date: Sat, 25 Oct 2014 19:09:43 +0200
Subject: [PATCH] Switched to single platform RTC driver - AB500

---
 arch/arm/configs/cyanogenmod_golden_defconfig |    6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/arch/arm/configs/cyanogenmod_golden_defconfig b/arch/arm/configs/cyanogenmod_golden_defconfig
index 297ea03..287a9da 100644
--- a/arch/arm/configs/cyanogenmod_golden_defconfig
+++ b/arch/arm/configs/cyanogenmod_golden_defconfig
@@ -359,7 +359,7 @@ CONFIG_DB8500_MLOADER=y
 CONFIG_UX500_L2X0_PREFETCH_CTRL=y
 CONFIG_UX500_DB_DUMP=y
 # CONFIG_UX500_DEBUG_LAST_IO is not set
-CONFIG_RTC_HCTOHC=y
+# CONFIG_RTC_HCTOHC is not set
 CONFIG_UX500_HW_OBSERVER=y
 # CONFIG_U8500_CUSTOM_RF is not set
 # CONFIG_BT_BCM4330 is not set
@@ -2322,7 +2322,7 @@ CONFIG_RTC_INTF_DEV=y
 # CONFIG_RTC_INTF_DEV_UIE_EMUL is not set
 CONFIG_RTC_INTF_ALARM=y
 CONFIG_RTC_INTF_ALARM_DEV=y
-CONFIG_RTC_ALARM_DEV_NAME="pl031"
+# CONFIG_RTC_ALARM_DEV_NAME is not set
 # CONFIG_RTC_DRV_TEST is not set
 
 #
@@ -2383,7 +2383,7 @@ CONFIG_RTC_DRV_AB8500=y
 # on-CPU RTC drivers
 #
 # CONFIG_RTC_DRV_PL030 is not set
-CONFIG_RTC_DRV_PL031=y
+# CONFIG_RTC_DRV_PL031 is not set
 CONFIG_DMADEVICES=y
 # CONFIG_DMADEVICES_DEBUG is not set
 
